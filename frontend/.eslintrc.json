{
    "env": {
    "browser": true,
    "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "plugin:react/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": ["./tsconfig.json"],
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "arrow-body-style": "off",
        "react/jsx-one-expression-per-line": "off",
        "react/require-default-props": "off",
        "react/jsx-props-no-spreading": "off"
    }
}
